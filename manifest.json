{
  "name": "Google GA",
  "description": "An example minimal ship",
  "version": "0.0.1",
  "tags": [
    "incoming",
    "oneColumn"
  ],
  "settings": [],
  "private_settings": [
    {
      "name": "refresh_token",
      "type": "string",
      "format" : "hidden"
    },
    {
      "name": "access_token",
      "type": "string",
      "format" : "hidden"
    },
    {
      "name" : "view_id",
      "description" : "View ID",
      "type" : "string"
    },
    {
      "name" : "dimensions",
      "description" : "Dimensions",
      "type" : "array",
      "items": { "type" : "string" },
      "format" : "select",
      "options": {
        "allowCreate" : true
      },
      "default": ["source", "medium", "channelGrouping", "adContent", "campaign"]
    }
  ],
  "readme": "readme.md",
  "admin": "/auth/",
  "ui": false,
  "schedules": [{
      "url": "/sync",
      "type": "cron",
      "value": "*/1 * * * *"
    }]
}
