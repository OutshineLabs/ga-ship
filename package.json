{
  "name": "Google Analytics Ship",
  "description": "GA <-> Hull",
  "version": "0.1",
  "homepage": "https://github.com/OutshineLabs/ga-ship",
  "license": "MIT",
  "author": {
    "name": "Colin White",
    "email": "colinwhite93@gmail.com",
    "url": "https://github.com/MaybeColin"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/outshinelabs/ga-ship"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "babel server -d build",
    "start": "COMBINED=true node build",
    "start:dev": "COMBINED=true babel-watch -L server",
    "test": "npm run test:lint",
    "test:lint": "eslint server",
    "test:modules": "npm outdated --depth=0",
    "clean": "rimraf dist; rimraf build",
    "update": "updtr",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "webhooks",
    "integration",
    "user",
    "sync"
  ],
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-watch": "^2.0.2",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "updtr": "^0.2.1"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "express": "^4.14.1",
    "googleapis": "^17.1.0",
    "hull": "0.11.0-beta.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-hubspot": "github:unity/passport-hubspot"
  }
}
